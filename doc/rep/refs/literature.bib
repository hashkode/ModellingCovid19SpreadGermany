  keywords = {Computer simulation,covid-19,Deterministic model,model,Parameter estimation,Sensitivity analysis,Stability},
  file = {/Users/Tobias/Documents/zotero/storage/YHYYKN8I/Memon et al. - 2021 - Assessing the role of quarantine and isolation as .pdf}
