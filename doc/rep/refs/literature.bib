@article{pintonetoMathematicalModelCOVID192021,
  title = {Mathematical Model of {{COVID}}-19 Intervention Scenarios for {{S\~ao Paulo}}\textemdash{{Brazil}}},
  author = {Pinto Neto, Osmar and Kennedy, Deanna M. and Reis, Jos{\'e} Clark and Wang, Yiyu and Brizzi, Ana Carolina Brisola and Zambrano, Gustavo Jos{\'e} and {de Souza}, Joabe Marcos and Pedroso, Wellington and {de Mello Pedreiro}, Rodrigo Cunha and {de Matos Brizzi}, Bruno and Abinader, Ellysson Oliveira and Z{\^a}ngaro, Renato Amaro},
